<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - SFCore.FineTuner.Web</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    @* <link rel="stylesheet" href="~/SFCore.FineTuner.Web.styles.css" asp-append-version="true" /> *@
</head>
<body>
    <header>
  <nav class="navbar navbar-dark navbar-dark-custom navbar-expand-lg">
        <div class="container-fluid">
            <a class="navbar-brand navbar-brand-gradient" href="/">SFCore.FineTuneX.ChatGPT</a>
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarNav">
                <ul class="navbar-nav ms-auto">
                    <li class="nav-item">
                        <a class="nav-link text-light" asp-controller="Home" asp-action="SetupModel">New Setup Training</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link text-light" asp-controller="Home" asp-action="privacy">Dataset</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link text-red red" asp-controller="Home" asp-action="SetupModel">New Setup Training</a>
                    </li>
                </ul>
            </div>
        </div>
    </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container footer-brand-gradient">
            &copy; @Html.Raw(DateTime.Now.Year) - SFCore.FineTuneX.ChatGPT - <label id="current-time"></label>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
